{
  "expo": {
    "name": "Ya Fue",
    "slug": "ya-fue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markajodi.bolt-expo-nativewind",
      "deploymentTarget": "15.1",
      "buildConfiguration": {
        "enableSwiftLibraryEvolution": true,
        "swiftCompilerFlags": [
          "-enable-library-evolution"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.markajodi.boltexponativewind"
    },
    "splash": {
      "image": "./assets/images/icon-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C0C0C"
    }
  }
}
